<Window x:Class="BudgetButikker.WinCategoryDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Tilføj Kategori" Height="130.4" Width="231.2" ResizeMode="NoResize" Icon="NEW ICON.ico">
    <Grid>
        <Label Name="lblWhat" Content="Kategori:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox Name="txtCategory" HorizontalAlignment="Left" Height="23" Margin="10,36,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="197"/>
        <Button Name="btnAddCategory" Content="Tilføj" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top" Width="92" Click="Button_Click"/>
        <Button Name="btnCancel" Content="Anuller" HorizontalAlignment="Left" Margin="115,64,0,0" VerticalAlignment="Top" Width="92" Click="Button_Click"/>

    </Grid>
</Window>
